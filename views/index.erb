<%galleries.each do |gallery|%>
    <h1><%=gallery.title%></h1>
    <section>
        <%gallery.photos.each do |photo|%>
            <article>
                <img src="<%=photo.url%>" />
                <figcaption><%=photo.likes%></figcaption>
                <img src="<%=photo.photographer.avatar%>" />
                <figcaption><%=photo.photographer.name%></figcaption>
                
            </article>
        <%end%>
    </section>
<%end%>